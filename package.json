{
  "name": "klan",
  "version": "0.0.1",
  "description": "Koa, LokiJS, Angular 2 and Node application",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rimraf dist",
    "clean-dev": "rimraf dev && rm -f npm-debug.log*",
    "build-client": "webpack --config src/client/config/webpack.prod.babel.js --progress",
    "build-server": "tsc --project src/server/ --outDir dist/",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "build-dev-client": "webpack --config src/client/config/webpack.dev.babel.js --progress",
    "build-dev-server": "tsc --project src/server/ --outDir dev/ --sourceMap",
    "build-dev": "npm run clean-dev && npm run build-dev-server & npm run build-dev-client",
    "dev": "npm run build-dev && nodemon --watch dev/server dev/server/app.js & npm run watch",
    "livereload": "$(npm bin)/browser-sync start --proxy 'http://localhost:3000' --files 'dev/client/**/*'",
    "start": "node dist/server/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run watch-dev-client & npm run watch-dev-server",
    "watch-dev-client": "webpack --config src/client/config/webpack.dev.babel.js --progress --watch",
    "watch-dev-server": "tsc --project src/server/ --outDir dev/ --sourceMap --watch"
  },
  "keywords": [
    "koa",
    "lokijs",
    "angular2",
    "node",
    "javascript"
  ],
  "author": "Bruno Krebs",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "2.5.35",
    "@types/jsonwebtoken": "7.1.33",
    "@types/koa": "2.0.39",
    "@types/koa-bodyparser": "3.0.23",
    "@types/koa-router": "7.0.22",
    "@types/koa-static": "2.0.22",
    "@types/lokijs": "1.2.30",
    "@types/node": "6.0.45",
    "angular2-template-loader": "0.4.0",
    "awesome-typescript-loader": "2.2.4",
    "babel-cli": "6.18.0",
    "babel-preset-latest": "6.16.0",
    "browser-sync": "2.18.2",
    "browsersync": "0.0.1-security",
    "css-loader": "0.23.1",
    "css-to-string-loader": "0.1.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.15.0",
    "node-sass": "4.0.0",
    "nodemon": "1.11.0",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.7",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "1.6.1",
    "rimraf": "2.5.2",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "typescript": "2.1.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.14.0"
  },
  "dependencies": {
    "@angular/common": "2.2.0",
    "@angular/compiler": "2.2.0",
    "@angular/core": "2.2.0",
    "@angular/forms": "2.2.0",
    "@angular/http": "2.2.0",
    "@angular/platform-browser": "2.2.0",
    "@angular/platform-browser-dynamic": "2.2.0",
    "@angular/router": "3.2.0",
    "angular2-jwt": "0.2.2",
    "auth0-lock": "10.7.3",
    "bootstrap-sass": "3.3.7",
    "cerialize": "0.1.13",
    "core-js": "2.4.1",
    "jsonwebtoken": "7.3.0",
    "jwks-rsa": "1.1.1",
    "koa": "2.2.0",
    "koa-bodyparser": "2.5.0",
    "koa-jwt": "^2.2.4",
    "koa-router": "^7.1.1",
    "koa-static": "^2.0.0",
    "lokijs": "1.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.25"
  }
}
